<UserControl
    x:Class="GesNautico.UI.Pages.SettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mui="http://firstfloorsoftware.com/ModernUI"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Views/Settings/Appearence.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Aparência" Source="/Views/Settings/Appearence.xaml" />
                <mui:Link DisplayName="Sobre" Source="/Views/Settings/About.xaml" />
            </mui:ModernTab.Links>

        </mui:ModernTab>
    </Grid>
</UserControl>
